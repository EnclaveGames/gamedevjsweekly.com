---
import '../styles/global.sass'

const { title, desc, image } = Astro.props
---
<!doctype html>
<html lang=en>
<head>
  <title>{(title ? title + ' | ' : '') + 'Gamedev.js Weekly'}</title>
  <meta name=description content={desc || 'Subscribe to the free, weekly newsletter about web game development. Curated by Andrzej Mazur, creator of the js13kGames competition.'}>
  <meta name=viewport content='width=device-width'>
  <meta name=theme-color content=#9bd>
  <meta property=og:site_name content='Gamedev.js Weekly'>
  <meta property=og:title content={title || 'Gamedev.js Weekly'}>
  <meta property=og:type content={image ? 'article' : 'website'}>
  <meta property=og:image content={image ?? '//gamedevjsweekly.com/gamedevjsweekly.png'}>
  <meta property=twitter:card content=summary_large_image>
  <link rel=icon href=/fav.svg>
  <link rel=alternate title='Gamedev.js Weekly' type=application/rss+xml href=feed.xml>
  <link rel=me href='//mastodon.gamedev.place/@GamedevJS'>
</head>
<script is:inline>new FontFace('a','url(data:application/octet-stream;base64,)',{weight:'1 900'}).load().then(s=>document.fonts.add(s))</script>
