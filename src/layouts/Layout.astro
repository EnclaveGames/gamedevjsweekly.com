---
import '../styles/global.sass'

const { title, image } = Astro.props
---
<!doctype html>
<html lang=en>
<head>
  <title>{title ? title + ' | ' : ''} Gamedev.js Weekly</title>
  <meta name=description content='Subscribe to the free, weekly newsletter about web game gevelopment. Curated by Andrzej Mazur, creator of the js13kGames competition.'>
  <meta name=viewport content='width=device-width'>
  <meta name=theme-color content=#9bd>
  <meta property=og:site_name content='Gamedev.js Weekly'>
  {image &&
      <meta property=og:type content=article>
      <meta property=og:image content={image}>
      <meta property=twitter:card content=summary_large_image>
  }
  <link rel=icon href="/fav.svg">
</head>
<script is:inline>new FontFace('a', 'url(data:application/octet-stream;base64,)', {weight: '1 900'}).load().then(s => document.fonts.add(s))</script>
